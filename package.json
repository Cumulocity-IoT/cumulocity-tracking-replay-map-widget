  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildPatch": "cd projects/gp-tracking-replay-map && npm version patch && ng build gp-tracking-replay-map && cd ../../  && cd ./dist/gp-tracking-replay-map && npm pack && move *.tgz ../",
    "buildMinor": "cd projects/gp-tracking-replay-map && npm version minor && ng build gp-tracking-replay-map && cd ../../ && cd ./dist/gp-tracking-replay-map && npm pack && move *.tgz ../",
    "buildMajor": "cd projects/gp-tracking-replay-map && npm version major && ng build gp-tracking-replay-map && cd ../../ && cd ./dist/gp-tracking-replay-map && npm pack && move *.tgz ../",
    "serve": "ng build gp-tracking-replay-map && npm i dist/gp-tracking-replay-map && ng s",
    "runtime": "gulp --gulpfile ./runtime/gulpfile.js"
  },
  "private": true,
  "dependencies": {
